{
  "name": "doob",
  "version": "0.9.1",
  "description": "Smart immutable state for React",
  "keywords": [
    "react",
    "react-component",
    "state",
    "immutable",
    "flux",
    "data"
  ],
  "homepage": "https://github.com/mistadikay/doob#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistadikay/doob.git"
  },
  "contributors": [
    "Denis Koltsov <iam@mistadikay.com> (https://github.com/mistadikay)",
    "Kir Belevich <kir@soulshine.in> (https://github.com/deepsweet)"
  ],
  "main": "build/index.js",
  "files": [
    "build/",
    "LICENSE"
  ],
  "dependencies": {
    "react": "0.13.x",
    "babel-runtime": "5.8.x",
    "baobab": "2.0.0-rc1",
    "emmett": "3.1.x",
    "lodash.isequal": "3.0.x"
  },
  "devDependencies": {
    "husky": "0.9.x",

    "babel": "5.8.x",
    "babel-core": "5.8.x",
    "babel-plugin-typecheck": "1.2.x",

    "gulp": "gulpjs/gulp#4.0",
    "del": "1.2.x",

    "webpack": "1.11.x",
    "webpack-dev-server": "1.10.x",
    "babel-loader": "5.3.x",
    "isparta-loader": "0.2.x",

    "mocha": "2.2.x",
    "chai": "3.2.x",
    "chai-spies": "0.7.x",
    "coveralls": "2.11.x",

    "karma": "0.13.x",
    "karma-mocha": "0.2.x",
    "karma-mocha-reporter": "1.1.x",
    "karma-chrome-launcher": "0.2.x",
    "karma-firefox-launcher": "0.1.x",
    "karma-webpack": "1.7.x",
    "karma-coverage": "0.5.x",
    "karma-clear-screen-reporter": "0.0.x",

    "eslint": "1.1.x",
    "eslint-plugin-babel": "2.1.x",
    "eslint-plugin-react": "3.2.x",
    "babel-eslint": "4.0.x"
  },
  "scripts": {
    "build": "rm -rf build/ && babel lib/ -d build/",
    "dev": "npm run build -- -w",

    "gulp": "babel-node --harmony ./node_modules/.bin/gulp --gulpfile test/gulpfile.es6",
    "lint": "eslint --ext .es6 lib/ test/",
    "test-build": "npm run gulp test:build",
    "test-dev": "npm run gulp test:dev",
    "test-travis": "npm run gulp test:travis",

    "coveralls": "cat coverage/lcov.info | coveralls",
    "travis": "NODE_ENV=test npm run lint && npm run test-travis && npm run coveralls",

    "test": "NODE_ENV=test npm run lint && npm run test-build",
    "prepush": "npm run test",
    "prepublish": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/mistadikay/doob/issues"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=2.7.0"
  },
  "license": "MIT"
}
