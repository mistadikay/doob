{
  "name": "doob",
  "version": "1.0.1",
  "description": "Smart immutable state for React",
  "keywords": [
    "react",
    "react-component",
    "state",
    "immutable",
    "flux",
    "data"
  ],
  "homepage": "https://github.com/mistadikay/doob#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistadikay/doob.git"
  },
  "contributors": [
    "Denis Koltsov <iam@mistadikay.com> (https://github.com/mistadikay)",
    "Kir Belevich <kir@soulshine.in> (https://github.com/deepsweet)"
  ],
  "main": "build/index.js",
  "files": [
    "build/",
    "LICENSE"
  ],
  "peerDependencies": {
    "react": ">=0.13.0 <0.15.0"
  },
  "dependencies": {
    "babel-runtime": "5.8.x",
    "baobab": "2.0.x",
    "lodash.isequal": "3.0.x"
  },
  "devDependencies": {
    "react": "^0.13.0",

    "husky": "0.10.x",
    "rimraf": "2.4.x",

    "babel": "5.8.x",
    "babel-core": "5.8.x",
    "babel-plugin-typecheck": "1.3.x",

    "webpack": "1.12.x",
    "webpack-dev-server": "1.12.x",
    "babel-loader": "5.3.x",
    "isparta-loader": "1.0.x",

    "mocha": "2.3.x",
    "chai": "3.3.x",
    "chai-spies": "0.7.x",
    "coveralls": "2.11.x",

    "karma": "0.13.x",
    "karma-mocha": "0.2.x",
    "karma-mocha-reporter": "1.1.x",
    "karma-chrome-launcher": "0.2.x",
    "karma-firefox-launcher": "0.1.x",
    "karma-webpack": "1.7.x",
    "karma-coverage": "0.5.x",
    "karma-clear-screen-reporter": "0.0.x",

    "eslint": "1.5.x",
    "eslint-plugin-babel": "2.1.x",
    "eslint-plugin-react": "3.5.x",
    "babel-eslint": "4.1.x"
  },
  "scripts": {
    "prebuild": "rimraf build/",
    "build": "babel lib/ -d build/",

    "dev": "npm run build -- -w",
    "tdd": "npm run karma start test/karma.dev.js",

    "lint": "eslint lib/ test/",

    "prekarma": "rimraf coverage/",
    "karma": "babel-node ./node_modules/.bin/karma",

    "precoveralls": "npm run karma start test/karma.travis.js",
    "coveralls": "coveralls < coverage/lcov.info",

    "pretravis": "npm run lint",
    "travis": "npm run coveralls",

    "pretest": "npm run lint",
    "test": "npm run karma start test/karma.build.js",

    "prepush": "npm test",
    "prepublish": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/mistadikay/doob/issues"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=2.7.0"
